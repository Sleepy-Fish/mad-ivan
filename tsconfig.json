{ 
  "compilerOptions": {
    "outDir": "./public/",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "module": "es6",
    "moduleResolution": "node",
    "target": "es5",
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "@": [ "src/*" ],
      "@/*": [ "src/*" ],
      "@src/*": [ "src/*" ],
      "@static/*": [ "static/*" ],
      "@assets/*": [ "assets/*" ],
    }
  }
}
